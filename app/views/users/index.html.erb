<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% @users.each do |user| %>
	<p>
		<%= user.first_name %>
		<%= user.last_name %>
		
	</p>
	<ul id="hello"><%= user.email %></ul>

	<%= link_to "#{user.full_name} Show", user %> 
	<%= link_to "Edit User", edit_user_path(user) %>
	<%= link_to "Destroy", user, method: :delete%>
<% end %>
