<h1>Sessions#new</h1>
<% if flash[:alert] %>
<p><%= flash[:alert] %></p>
<% end %>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_with url:sessions_path, scope: 'login' do |f| %>
	<%= f.email_field :email, placeholder: 'Email' %>
	<%= f.password_field :password, placeholder: 'Password' %>
	<%= f.submit "Log In" %>
<% end %>